{
    "name": "vigihdev/wp-cli-tools",
    "description": "A collection of tools to help with wp cli command line utilities",
    "homepage": "https://github.com/vigihdev/wp-cli-tools",
    "keywords": [
        "wordpress",
        "wp-cli",
        "cli",
        "tools",
        "development",
        "toolkit",
        "oop",
        "design-patterns"
    ],
    "type": "wp-cli-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "your-email@example.com",
            "homepage": "https://github.com/vigihdev",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/filesystem": "^6.4",
        "vigihdev/support": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "symfony/dotenv": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpCliTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpCliTools\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit",
        "watch": "nodemon dev.js",
        "build": "console git:push && console satis:build && cd $HOME/.wp-cli/packages && console composer:update vigihdev/wp-cli-tools && cd $HOME/WordpressCli/wp-cli-tools",
        "dev": "php -S localhost:3272 dev.php"
    }
}